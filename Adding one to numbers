#include<iostream>
using namespace std;

int main(){
	
	int a,b,c,d;
	
	cout<<"type ur first digit :\n";
	cin>>a;
	
	cout<<"type ur second digit :\n";
	cin>>b;
	
	cout<<"type ur third digit :\n";
	cin>>c;
	
	cout<<"type ur fourth digit :\n";
	cin>>d;
	
	a = a+1;
	b = b+1;
	c = c+1;
	d = d+1;
	
	cout<<"the numbers are :"<<a<<b<<c<<d;
	
	
}
